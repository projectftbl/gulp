{
  "parser": "babel-eslint"
, "extends": "eslint:recommended"
, "env": {
    "browser": true
  , "node": true
  }
, "rules": {
    "strict": 0
  }
, "plugins": [
    "react"
  ]
, "ecmaFeatures": {
    "jsx": true
  }
, "rules": {
    "comma-style": [ 2, "first" ]
  , "no-unused-vars": [ 2, { "args": "after-used", "varsIgnorePattern": "React" }]
  }
}